{
  "private": true,
  "scripts": {
    "start": "cross-env UMI_ENV=dev umi dev",
    "build": "cross-env UMI_ENV=prod HTML=none umi build",
    "block": "cross-env UMI_UI=1 UMI_ENV=block umi dev",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "@ant-design/pro-layout": "^6.5.0",
    "ahooks": "^2.10.9",
    "classnames": "^2.3.1",
    "express": "^4.17.1",
    "marked": "^3.0.4",
    "moment": "^2.29.1",
    "numeral": "^2.0.6",
    "nzh": "^1.0.5",
    "react": "17.x",
    "react-dom": "17.x",
    "react-json-view": "^1.21.3",
    "remark": "^13.0.0",
    "umi": "^3.5.18"
  },
  "devDependencies": {
    "@types/marked": "^3.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.5.18",
    "cross-env": "^7.0.3",
    "html-loader": "^2.1.2",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "remark-loader": "^4.0.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
