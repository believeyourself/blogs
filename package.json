{
  "private": true,
  "scripts": {
    "start": "cross-env UMI_ENV=dev umi dev",
    "build": "cross-env UMI_ENV=prod HTML=none umi build",
    "build:dev": "cross-env UMI_ENV=dev HTML=none umi build",
    "block": "cross-env UMI_UI=1 UMI_ENV=block umi dev",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "analyze": "cross-env UMI_ENV=prod ANALYZE=1 umi build"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-layout": "^6.38.5",
    "ahooks": "^2.10.14",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.2",
    "express": "^4.18.1",
    "js-md5": "^0.7.3",
    "marked": "^3.0.8",
    "numeral": "^2.0.6",
    "nzh": "^1.0.8",
    "qnn-react-cron": "^0.1.3",
    "react": "17.x",
    "react-adsense": "^0.1.0",
    "react-dom": "17.x",
    "react-json-view": "^1.21.3",
    "react-pdf": "^5.7.2",
    "react-photo-view": "^0.5.7",
    "remark": "^13.0.0",
    "umi": "^3.5.24"
  },
  "devDependencies": {
    "@types/js-md5": "^0.4.3",
    "@types/marked": "^3.0.3",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@umijs/preset-react": "^1.8.31",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.5.24",
    "cross-env": "^7.0.3",
    "html-loader": "^2.1.2",
    "lint-staged": "^10.0.7",
    "prettier": "^2.6.2",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "remark-loader": "^4.0.0",
    "typescript": "^4.7.2",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "@ant-design/icons": "^4.7.0",
    "@babel/runtime": "^7.11.2"
  }
}
